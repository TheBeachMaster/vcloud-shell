function api_command_org_metadata()
{
	if [ $# -ne 1 ]; then
		exitError "api ${FUNCNAME[0]:12} requires <orgName>"
	fi
	
	local -r orgName="$1"
	
	local curl_downloadFilePath
	api_makeCallAndFindHrefInLinksAndGet links api_callbackGetHrefFromMimeType metadata "application/vnd.vmware.vcloud.metadata+xml" '' \
		api_command_admin "$orgName"
	
	api_format_metadata "${FUNCNAME[0]:12}"
}
Functions+=(org_metadata)
