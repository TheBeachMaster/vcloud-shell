# eg ./vcloud-curl -f id-tsv vdc 98-184-1-151d73 'Student Loans Company - Platform Engineering (IL0-TRIAL-BASIC)'
function api_vdc_network()
{
	if [ $# -ne 3 ]; then
		exitError "command vcd_network requires <orgName> <vdcName> <networkName>"
	fi
	
	local -r orgName="$1"
	local -r vdcName="$2"
	local -r networkName="$2"
	
	temporaryFiles_newFileToRemoveOnExit
	local -r adminTsvFile="$TMP_FILE"
	switch_format=links-vdcs switch_outputHeadingLine=0 api_vdc "$orgName" "$vdcName" >"$adminTsvFile"
}