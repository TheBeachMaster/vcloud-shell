function api_vdc_metadata()
{
	if [ $# -ne 2 ]; then
		exitError "command ${FUNCNAME[0]:4} requires <orgName> <vdcName> <networkName>"
	fi
	
	local -r orgName="$1"
	local -r vdcName="$2"
	
	local curl_downloadFilePath
	api_makeCallAndFindHrefInLinksAndGet links api_findMimeTypeHrefInLinks metadata "application/vnd.vmware.vcloud.metadata+xml" \
		api_vdc "$orgName" "$vdcName"
	
	api_formatMetadata
}
api_commandLineFunctions+=(vdc_metadata)
